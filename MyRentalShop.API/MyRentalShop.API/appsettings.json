{

  "ConnectionStrings": {
    "MyRentalShopApiDatabase": "Server=localhost\\SQLExpress;Database=MyRentalShopApiDatabase;Trusted_Connection=True;"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ]

  },
  "AllowedHosts": "*"
}
